/*****************************************************************************/
/* CustomSets: Style */
/*****************************************************************************/
.custom_sets {
}

.title {
  margin: 0;
}

.content {
  margin-top: 20px;
}

.searchInput {
  margin-top: 15px;
  margin-bottom: 15px;
}

.sorting {
  position: relative;
  width: 100%;
  height: 34px;
}

.notFound {
  font-size: 2em;
  margin: 20px;
}

.itemSet {
  background-color: rgb(255, 201, 152);
}

.itemSet .heading {
  background-color: rgb(242, 163, 91);
}

.itemSet .title {
  font-weight: bold;
  font-size: 24px;
  line-height: 24px;
}

.itemSet .champNameRole {
  font-weight: bold;
  font-size: 1.7em;
}

.itemSet .headRating__votes {
  height: 24px;
  font-size: 1em;
  line-height: 24px;
  margin-right: 8px;
}

.itemSet .headRating__stars {
  float: right;
  text-align: right;
}

.itemSet .champImage {
  width: 150px;
}

.itemSet .author {
  font-size: 1.3em;
}

.itemSet .creationDate {
}

.itemSet .downloadSection {
  margin-top: 10px;
}

.itemSet .downloadButton {
  width: 100%;
}

.itemSet .userRating {
  font-weight: bold;
  background-color: rgb(242, 163, 91);
  padding: 5px;
}

.itemSet .itemSet__rating {
  text-align: center;
}

.itemSet .itemSet__ratingButton {
  width: 100%;
}

.itemSet .itemSetContent {
  position: relative;
  height: 100%;
}

.itemSet .reportContainer {
  float: right;
}

.itemSet .itemsList {
}

.itemSet .block {
  margin-top: 8px;
  margin-bottom: 8px;
}

.itemSet .blockTitle {
  font-weight: bold;
}

.itemSet .blockItems {
}

.itemSet .item {
  display: inline;
  width: 50px;
  margin-bottom: 5px;
}

.itemSet .comment {
  margin: 0 30px;
  font-size: 1.4em;
}

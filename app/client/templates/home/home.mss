/*****************************************************************************/
/* Home: Style */
/*****************************************************************************/
.home {
}

.resume {
  display: flex;
  font-size: 2.0em;
}

.resume a {
  text-align: center;
}

.description {
  margin: auto;
}

.browse_sets {
  line-height: 50px;
  height: 50px;
  margin-top: 18px;
  font-size: 2em;
  border: 1px solid #E0E0E0;
  background: url(img/sets_button.jpg);
  width: 100%;
  height: 100%;
}

.browse_sets a {
  color: #E0E0E0;
  background-color: rgba(56, 56, 56, 0.2);
  font-weight: bold;
  text-decoration: none;
  display: block;
  width: 100%;
  height: 100%;
}

.browse_sets a:hover {
  background-color: rgba(56, 56, 56, 0.5);
}

.browse_sets a span {
  background-color: rgba(56, 56, 56, 0.6);
  -webkit-box-shadow: 0px 0px 8px 5px rgba(56, 56, 56, 0.6);
  -moz-box-shadow: 0px 0px 8px 5px rgba(56, 56, 56, 0.6);
  box-shadow: 0px 0px 8px 5px rgba(56, 56, 56, 0.6);
  padding: 3px 8px;
}

.download {
  margin-top: 30px;
}

@media (max-width: 767px) {
  .downloadSection {
    margin-top: 20px;
  }
}

.download_button {
  font-size: 1.8em;
}

.itemSetsPath {
  padding: 15px 5px;
}

.app_preview {
  max-height: 250px;
}

.preview {
}

.previewImage {
}

.faq {
}

#newsfeed {
  text-align: center;
}

#partners {
  text-align: center;
  margin-top: 40px;
  margin-bottom: 40px;
}

#partners h2 {
  margin-bottom: 40px;
}

.partner {
  overflow: auto;
  text-align: left;
  margin-top: 10px;
  margin-bottom: 10px;
}

.partner a {
  float: left;
  width: 20px;
  margin-left: 80px;
}

.partner img {
  width: 220px;
  height: 200px;
}

.partner_description {
  height: auto;
  margin-left: 300px;
  margin-top: 50px;
  padding-left: 30px;
}
